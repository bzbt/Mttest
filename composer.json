{
  "name": "bzbt/calls-report",
  "type": "project",
  "license": "MIT",
  "description": "MT Test",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "config/configuration.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/App"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ext-curl": "^7.4",
    "ext-fileinfo": "*",
    "ext-openssl": "*",
    "doctrine/annotations": "^1.12",
    "doctrine/cache": "^1.10",
    "symfony/framework-bundle": "^5.2",
    "symfony/mime": "^5.2",
    "symfony/routing": "^5.2"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "phpstan/phpstan": "^0.12.81",
    "phpunit/phpunit": "^9.5"
  }
}
